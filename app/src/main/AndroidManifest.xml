<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.frontend.classapp.org.classapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE"></uses-permission>

    <application

        android:allowBackup="true"
        android:icon="@drawable/imagen"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">



        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>




        <activity android:name=".MainActivity" android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.DetailActivity" />


            </intent-filter>
        </activity>

        <activity android:name=".DetailActivity" android:parentActivityName=".MainActivity"  />


        <uses-permission android:name="android.permission.INTERNET" />




        <activity android:name=".aboutActivity" android:parentActivityName=".SplashActivity"></activity>

        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings"></activity>





    </application>

</manifest>











